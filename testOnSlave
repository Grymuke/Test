pipeline {
	agent {label 'SlaveServer'
  }
    stages {
        stage('Unzip') {
            steps {
	    	sh 'whoami'
                echo 'Unzip file....'
                sh 'java -version'
                sh 'pwd'
		
		parentdir_name='test'
		if ( -d $parentdir_name )
                    then
                        echo $"Directory already exists"
                    else
                        sh 'mkdir $parentdir_name'
                fi
		
		dir ($parentdir_name) {
		    stampdir_name=$(date +%Y%m%d_%H%M%S)
		    sh 'mkdir stampdir_name'
    		}
		
            }
        }
    }
    //post {
	//always {
		//cleanWs()
		//}
	//}
}
